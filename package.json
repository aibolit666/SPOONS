{
  "name": "spoons",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://aibolit666.github.io/spoons",
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@reduxjs/toolkit": "^2.2.5",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-prettier": "^5.1.3",
    "babel-jest": "^29.0.0",
    "ts-jest": "^29.1.4",
    "gh-pages": "^6.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-css-modules": "^2.1.0",
    "lint-staged": "^15.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.5",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.22.3",
    "react-scripts": "^5.0.1",
    "sass": "^1.77.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build",
    "predeploy": "npm run build",
    "jest": "jest",
    "husky-install": "husky install",
    "lint": "eslint src --color"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jest.setup.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node",
      "jpg"
    ],
    "moduleNameMapper": {
      ".(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/utils/__mock__/file-mock.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/src/utils/__mock__/styleMock.js"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-react": "^7.34.0",
    "prettier": "3.2.5",
    "typescript": "^5.4.0"
  },
  "overrides": {
    "typescript": "^5.4.0"
  }
}